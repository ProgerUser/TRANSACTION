            <tooltip>
               <Tooltip text="Удалить">
                  <font>
                     <Font size="14.0" />
                  </font>
               </Tooltip>
            </tooltip></Button>
         <Separator layoutX="147.0" layoutY="10.0" orientation="VERTICAL" />
         <Button id="searchEmpBtn" layoutX="59.0" layoutY="10.0" mnemonicParsing="false" onAction="#Add" prefWidth="35.0">
