<BorderPane prefHeight="600.0" xmlns="http://javafx.com/javafx/8.0.261" xmlns:fx="http://javafx.com/fxml/1" fx:controller="trlist.Tr_Am_View_con">
	<center>
		<StackPane BorderPane.alignment="CENTER">
