                        <FontAwesomeIconView fontSmoothingType="LCD" glyphName="LIST_UL" size="15" />
                     </graphic>
                     <font>
                        <Font size="10.0" />
                     </font>
						</Button>
						<Button mnemonicParsing="false" onAction="#excel_export" fx:id="xlsx">
							<tooltip>
								<Tooltip text="Выгрузка в Excel">
                           <font>
                              <Font size="14.0" />
                           </font></Tooltip>
							</tooltip>
                     <graphic>
                        <FontAwesomeIconView fontSmoothingType="LCD" glyphName="FILE_EXCEL_ALT" size="15" />
                     </graphic>
                     <font>
                        <Font size="10.0" />
                     </font>
						</Button>
						<Button fx:id="post" mnemonicParsing="false" onAction="#view_post">
							<tooltip>
								<Tooltip text="Показать проводки">
                           <font>
                              <Font size="14.0" />
                           </font></Tooltip>
							</tooltip>
                     <graphic>
                        <FontAwesomeIconView fontSmoothingType="LCD" glyphName="TABLE" size="15" />
                     </graphic>
                     <font>
                        <Font size="10.0" />
                     </font>
						</Button>
						<Button fx:id="print_check" mnemonicParsing="false" onAction="#print_">
							<tooltip>
								<Tooltip text="Печать чека JasperReport">
                           <font>
                              <Font size="14.0" />
                           </font></Tooltip>
							</tooltip>
                     <graphic>
                        <FontAwesomeIconView fontSmoothingType="LCD" glyphName="PRINT" size="15" />
                     </graphic>
						</Button>
						<Button fx:id="chk_nmbers" ellipsisString="" mnemonicParsing="false" onAction="#showdeal">
							<tooltip>
								<Tooltip text="Показать сдачи">
                           <font>
                              <Font size="14.0" />
                           </font></Tooltip>
							</tooltip>
                     <graphic>
                        <FontAwesomeIconView fontSmoothingType="LCD" glyphName="LIST" size="15" />
                     </graphic>
						</Button>
						<Button fx:id="trn_" mnemonicParsing="false" onAction="#view_trn">
							<tooltip>
								<Tooltip text="Реестр документов">
                           <font>
                              <Font size="14.0" />
                           </font></Tooltip>
							</tooltip>
                     <graphic>
                        <FontAwesomeIconView fontSmoothingType="LCD" glyphName="LIST_OL" size="15" />
                     </graphic>
						</Button>
					</items>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
				</ToolBar>
				<ToolBar>
