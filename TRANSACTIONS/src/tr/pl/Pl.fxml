<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.291" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <children>
            <TitledPane alignment="CENTER" collapsible="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Пользователи" HBox.hgrow="ALWAYS">
               <content>
                  <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                     <children>
                        <TableView fx:id="UsrLst" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="300.0" VBox.vgrow="ALWAYS">
                          <columns>
                            <TableColumn fx:id="Login" prefWidth="75.0" text="Логин" />
                            <TableColumn fx:id="Fio" prefWidth="170.0" text="ФИО" />
                              <TableColumn fx:id="Stat" prefWidth="55.0" text="Стат." />
                          </columns>
                        </TableView>
                     </children>
                  </VBox>
               </content>
            </TitledPane>
            <TitledPane alignment="CENTER" collapsible="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Доступные счета" HBox.hgrow="ALWAYS">
               <content>
                  <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0">
                     <children>
                        <ToolBar VBox.vgrow="ALWAYS">
                          <items>
                            <Button mnemonicParsing="false" onAction="#Plus">
                                 <graphic>
                                    <FontAwesomeIconView fontSmoothingType="LCD" glyphName="PLUS" size="15" />
                                 </graphic>
                              </Button>
                              <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#Minus">
                                 <graphic>
                                    <FontAwesomeIconView fontSmoothingType="LCD" glyphName="TRASH_ALT" size="15" />
                                 </graphic>
                              </Button>
                          </items>
                           <VBox.margin>
                              <Insets left="-10.0" right="-10.0" top="-10.0" />
                           </VBox.margin>
                        </ToolBar>
                        <TableView fx:id="ExAcc" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
                           <columns>
                              <TableColumn fx:id="ExAccount" prefWidth="150.0" text="Счет" />
                              <TableColumn fx:id="ExFio" prefWidth="160.0" text="ФИО" />
                              <TableColumn prefWidth="75.0" text="Доступность">
                                 <columns>
                                    <TableColumn fx:id="D_START" prefWidth="120.0" text="Дата с" />
                                    <TableColumn fx:id="D_END" prefWidth="120.0" text="Дата по" />
                                 </columns>
                              </TableColumn>
                           </columns>
                        </TableView>
                     </children>
                  </VBox>
               </content>
            </TitledPane>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
